<!-- Ambra Configuration file -->
<config> 
  <ambra> 
    <network> 
      <hosts> 
        <!-- Set hosts.default to FQHN (not localhost) if installing all components on one host 
          (or to webhead FQHN for multiple host installation) --> 
        <default>plosone-jfinger</default> 
        <mailhost>localhost</mailhost> 
        <!-- For multibox install, change the following entires to the appropriate FQHNs --> 
        <cas>${ambra.network.hosts.default}</cas>
        <registration>${ambra.network.hosts.cas}</registration> 
        <fedora>${ambra.network.hosts.default}</fedora> 
        <mulgara>${ambra.network.hosts.default}</mulgara> 
        <ambra>${ambra.network.hosts.default}</ambra> 
      </hosts> 

      <!-- Ports for the various components. Default for HTTP and HTTPS can be left out --> 
      <ports> 
        <default>${ambra.network.ports.ambra}</default> 
        <cas-ssl>43</cas-ssl> 
        <mulgara>8111</mulgara> 
      </ports> 
    </network> 

    <platform> 
      <!-- common display constants --> 
      <copyright> 
        This work is licensed under a Creative Commons Attribution-Share Alike 3.0 License, 
        http://creativecommons.org/licenses/by-sa/3.0/ 
      </copyright> 
      <email> 
        <general>admin@plos.org</general> 
        <feedback>admin@plos.org</feedback> 
		<errors>admin@plos.org</errors>

      </email> 
      <freemarker>
        <almHost>http://alm.plos.org</almHost>
      </freemarker>
      <name>PLoS</name> 
      <guid-prefix>info:doi/0.0/</guid-prefix>
			<dummySsoEnabled>true</dummySsoEnabled>
    </platform> 

    <!-- 
      URI aliases. eg. id:annotation/... can be used in place of info:doi/10.1371/ 
      The main use for this is to define aliases that are used by id generators 
      and such that are configured with OTM. But could be used in other config 
      file sections to consolidate identifier prefix usages. 
    --> 
    <aliases> 
      <doiPrefix>info:doi</doiPrefix> 
      <id>${ambra.aliases.doiPrefix}/10.1371/</id> 
      <annoteaBodyId>info:fedora/</annoteaBodyId> 
    </aliases> 

    <!-- Overlay journal config --> 
    <virtualJournals> 
      <templateDir>/home/jfinger/templates</templateDir>

      <default>PLoSDefault</default> 
      <journals>PLoSDefault,PLoSClinicalTrials,PLoSCollections,PLoSCompBiol,PLoSGenetics,PLoSNTD,PLoSONE,PLoSPathogens,PLoSBiology,PLoSMedicine</journals> 
      <PLoSDefault> 
        <url>http://ambra-jfinger:8080</url> 
      </PLoSDefault> 
      <PLoSClinicalTrials> 
        <url>http://clinicaltrials-jfinger:8080</url> 
      </PLoSClinicalTrials> 
      <PLoSCollections> 
        <url>http://ploscollections-jfinger:8080</url> 
      </PLoSCollections> 
      <PLoSCompBiol> 
        <url>http://ploscompbiol-jfinger:8080</url> 
    </PLoSCompBiol>

			<PLoSGenetics> 
        <url>http://plosgenetics-jfinger:8080</url> 
      </PLoSGenetics> 
      <PLoSNTD> 
        <url>http://plosntds-jfinger:8080</url> 
      </PLoSNTD> 
      <PLoSONE> 
        <url>http://plosone-jfinger:8080</url> 
      </PLoSONE> 
      <PLoSPathogens> 
        <url>http://plospathogens-jfinger:8080</url> 
      </PLoSPathogens> 
      <PLoSBiology> 
        <url>http://plosbiology-jfinger:8080</url> 
      </PLoSBiology> 
      <PLoSMedicine> 
        <url>http://plosmedicine-jfinger:8080</url> 
      </PLoSMedicine> 
    </virtualJournals> 

    <services> 
			 <database>

					<driver>com.mysql.jdbc.Driver</driver>
					<url>jdbc:mysql://ambradb/ambra</url>
					<username>ambra</username>
					<password>ambra</password>
      </database> 
      <filestore>
        <basedir>~/datastores/filestore/ambra</basedir>

      </filestore>
		<search> 
        <!-- Fields of lucene meta data that are searched by default --> 
        <defaultFields>description,title,body,creator</defaultFields> 
        <!-- Number of snippets to show per document --> 
        <snippetsMax>3</snippetsMax> 
        <!-- Max chars per indexed field? --> 
        <fieldMaxLength>50</fieldMaxLength> 
        <!-- Number of results per page --> 
        <pageSize>10</pageSize> 
				<server>

				
				<url>http://localhost:8983/solr</url>
				</server>
      </search> 

      <registration> 
        <!-- Default assumption is that the registration application is running on ambra-cas. If the 
        configuration is different, please set here --> 
        <appContext>/ambra-registration</appContext> 
      </registration> 

      <!-- defaults for generating an Atom Feed --> 
      <feed> 
        <title>PLoS ONE</title> 
        <tagline>Publishing science, accelerating research</tagline> 
        <icon>${webserver-url}images/favicon.ico</icon> 
        <!-- Unique id. Should change for different criteria --> 
        <id>info:doi/1371/feed.pone</id> 
        <extended> 
          <!-- namespace prefix for custom elements --> 
          <prefix>plos</prefix> 
          <!-- custom element namespace --> 
          <namespace>http://www.plos.org/atom/ns#plos</namespace> 
        </extended> 
        <!-- Path to feeds after journal context --> 
        <basePath>/article/</basePath> 
        <!-- Default feed if none other specified --> 
        <defaultFile>feed</defaultFile> 
        <!-- File prefix for a specific category --> 
        <categoryPrefix>feed?category=</categoryPrefix> 
        <!-- Name of default feed (prefixed by journal) --> 
        <defaultName>New Articles</defaultName> 
        <!-- Default duration in months --> 
        <defaultDuration>3</defaultDuration> 
      </feed>

      <documentManagement> 
	<ingestSourceDir>/root/ingest/</ingestSourceDir>
	<ingestDestinationDir>/root/ingested/</ingestDestinationDir>
	<auto_ingestSourceDir>/root/autoingest/</auto_ingestSourceDir>
	<auto_ingestDestinationDir>/root/ingested/</auto_ingestDestinationDir>
        <imageMagick> 
          <executablePath>/usr/bin/convert</executablePath> 
          <tempDirectory>/tmp</tempDirectory> 
          <imageSetConfigs> 
            <imageSet name="#default"> 
              <small width="70" quality="80"/> 
              <medium maxDimension="600" quality="80"/> 
              <large quality="80"/> 
            </imageSet> 
            <imageSet name="issueImage"> 
              <small width="120"/> 
            </imageSet> 
          </imageSetConfigs> 
        </imageMagick> 
      </documentManagement> 

      <!-- taxonomy -->

      <ai>
        <url>http://tax.plos.org:9080/servlet/dh</url>
        <thesaurus>plosthes.2013-5</thesaurus>
      </ai> 

      <!-- crossref information - set parameters and sendToXref to true to turn on --> 
      <crossref> 
				<xsl>crossref.xsl</xsl>

        <plos> 
          <doiurl> 
            http://dx.plos.org/ambra-doi-resolver/
          </doiurl> 
          <!-- email address to send info to --> 
          <email>plosdoi@plos.org</email> 
        </plos> 
        <!-- crossref login --> 
        <login>crossref-jfinger</login> 
        <!-- crossref password --> 
        <password>your-password</password> 
        <area>test</area> 
        <!-- Set to true to turn on crossref posting --> 
        <sendToXref>false</sendToXref> 
      </crossref> 

      <syndications>

        <!-- Pub Med Central syndication configuration -->
        <syndication target="PMC">
          <name>Pub Med Central</name>
          <queue>activemq:plos.pmc</queue>
          <responseQueue>activemq:plos.pmc.response</responseQueue>
        </syndication>
	        <!-- Crossref syndication configuration -->

        <syndication target="CROSSREF">
          <name>Crossref</name>
          <queue>activemq:plos.crossref</queue>
          <responseQueue>activemq:plos.crossref.response</responseQueue>
        </syndication>
      </syndications>
	  <browse>

		 <cache>true</cache>
		 <time>15</time>
	  </browse>


    <ehcache>
	    <configFileLocation>/etc/ambra/ehcache.xml</configFileLocation>
    </ehcache>


    </services> 
    
